{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "isolatedModules": true,
    "strict": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": false,
    "noImplicitAny": false
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}
